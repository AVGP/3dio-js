{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["lib", "package.json", "docs/api.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "../../tasks/jsdoc/plugins/embed-file",
    "../../node_modules/jsdoc/plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "docs/api/",
    "template": "tasks/jsdoc/template",
    "encoding": "utf8",
    "private": false,
    "recurse": true
  }
}